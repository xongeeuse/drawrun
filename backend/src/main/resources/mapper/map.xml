<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dasima.drawrun.domain.map.mapper.MapMapper">
    <resultMap id="UserPath" type="com.dasima.drawrun.domain.map.entity.UserPath">
        <id column="user_path_id" property="userPathId"/>
        <result column="user_id" property="userId"/>
        <result column="path_id" property="pathId"/>
    </resultMap>

    <insert id="pathsave">
        insert into user_path
        values (0, #{userId}, #{pathId})
    </insert>


</mapper>